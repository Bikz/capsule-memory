{
  "name": "@capsule/sdk-js",
  "version": "0.0.0",
  "description": "TypeScript SDK for Capsule Memory HTTP API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.17"
  }
}
